{
  "name": "util-ex",
  "description": "Browser-friendly enhanced util fully compatible with standard node.js",
  "homepage": "https://github.com/snowyu/util-ex.js",
  "version": "0.3.16",
  "author": {
    "name": "Riceball LEE",
    "email": "snowyu.lee@gmail.com"
  },
  "types": "./types",
  "files": [
    "README.md",
    "LICENSE-MIT",
    "*.js",
    "*.coffee",
    "test",
    "src",
    "types",
    "lib"
  ],
  "keywords": [
    "is",
    "validate",
    "util",
    "browser",
    "browserify"
  ],
  "main": "./lib/index.js",
  "repository": "git://github.com/snowyu/util-ex.js",
  "bugs": {
    "url": "https://github.com/snowyu/util-ex.js/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@antfu/eslint-config": "^0.37.0",
    "chai": "~4.3.7",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "mkdist": "^1.1.2",
    "mocha": "~10.2.0",
    "prettier": "^2.8.7",
    "sinon": "~15.0.3",
    "sinon-chai": "~3.7.0",
    "typedoc": "^0.23.28",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^5.0.2"
  },
  "pre-commit": [
    "test"
  ],
  "scripts": {
    "build": "npm run build.ts && mkdir -p lib && cp src/*.js lib/",
    "build.cjs": "mkdist --src=src --pattern=*.js --dist=lib --format=cjs",
    "build.mjs": "mkdist --src=src --pattern=*.js --dist=lib/esm --format=esm --ext=mjs",
    "clean": "rm -fr web docs types lib",
    "clean.doc": "rm -fr web docs",
    "clean.ts": "rm -fr types",
    "clean.lib": "rm -fr lib",
    "doc": "typedoc --plugin none --out web ./src",
    "doc.md": "typedoc --plugin typedoc-plugin-markdown --out docs ./src",
    "release": "npx standard-version",
    "release.alpha": "npx standard-version --prerelease alpha",
    "test": "mocha"
  },
  "dependencies": {
    "inherits-ex": "^1.5.0",
    "xtend": "^4.0.2"
  }
}
